Command For Create Two File :
          touch DATA1
          touch DATA2

Command For Insert Content In DATA1 :
          echo 'AB|CD|EF|GH' >  DATA1
          echo 'IJ|KL|MN|OP' >> DATA1
          echo 'QR|ST|UV|WX' >> DATA1
          echo 'YZ|12|34|56' >> DATA1
          echo '78|90|A1|B1' >> DATA1
          
Command For Insert Content In DATA2 :
          echo '10' > DATA2
          echo '4' >> DATA2
          echo '27' >> DATA2
          echo '27' >> DATA2
          echo '80' >> DATA2
          echo '80' >> DATA2
          echo '80' >> DATA2
          echo '4' >> DATA2
          echo '4' >> DATA2
          echo '99' >> DATA2
          echo '80' >> DATA2



#Question Number 1:
Command :
          head -n 4 DATA1 | tail -n 3
Output :
          IJ|KL|MN|OP
          QR|ST|UV|WX
          YZ|12|34|56
          
          
          
#Question Number 2:
Command :
          cut -d "|" -f 2,3 DATA1 | head -4 | tail -2
Output :
          ST|UV
          12|34
          
          
          
#Question Number 3:
Command :
          cut -d "|" -f 2,3 DATA1 | head -4 | tail -2
Output :
          10;AB|CD|EF|GH
          4;IJ|KL|MN|OP
          27;QR|ST|UV|WX
          27;YZ|12|34|56
          80;78|90|A1|B1
          80;
          80;
          4;
          4;
          99;
          80;
          
          
          
#Question Number 4:
Command :
